<mvc:View controllerName="lualbal.SAPUI5.controller.App"
          xmlns="sap.m"
          xmlns:mvc="sap.ui.core.mvc"
          xmlns:core="sap.ui.core"
          displayBlock="true">
    <Page title="{i18n>appTitle}">
            <headerContent>
                <Button id="helloDialogHeader"
                        icon="sap-icon://action"
                        press=".onOpenDialogHeader"
                        class="sapUiSmallMarginEnd"/>
            </headerContent>
            <content>
                <mvc:XMLView viewName="lualbal.SAPUI5.view.helloPanel"/>
                <mvc:XMLView viewName="lualbal.SAPUI5.view.InvoicesList"/>
            </content>
    </Page>
</mvc:View>